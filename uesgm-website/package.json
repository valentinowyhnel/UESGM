{
  "name": "uesgm-website",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "clean": "powershell -Command \"Remove-Item -Recurse -Force .next,node_modules/.prisma,node_modules/.cache -ErrorAction SilentlyContinue\"",
    "reinstall": "pnpm run clean && pnpm install && pnpm prisma generate",
    "build:dev": "next build",
    "dev": "next dev --webpack",
    "build": "set NEXT_TURBO=0 && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:api": "jest tests/api",
    "test:security": "node tests/scripts/security-test.js",
    "test:setup": "node tests/utils/test-helpers.js",
    "test:cleanup": "node -e \"require('./tests/utils/test-helpers.js').cleanupTestData()\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": ">=20.11"
  },
